{
  "name": "steven-mern-starter-kit",
  "version": "1.0.0",
  "description": "A Javascript stack starter kit",
  "main": "main.js",
  "author": "Chen Fang",
  "license": "Apache-2.0",
  "keywords": [
    "Node",
    "Koa",
    "React",
    "Redux",
    "Mongodb"
  ],
  "engines": {
    "node": ">=6.3.1",
    "npm": "^3.10.5"
  },
  "scripts": {
    "clean": "rm -rf public",
    "dev": "better-npm-run dev",
    "start": "better-npm-run start",
    "forever": "better-npm-run forever",
    "compile": "better-npm-run compile",
    "dev:no-debug": "npm run dev -- --no_debug",
    "test": "better-npm-run test",
    "test:dev": "npm run test -- --watch",
    "deploy": "better-npm-run deploy",
    "deploy:dev": "better-npm-run deploy:dev",
    "deploy:prod": "better-npm-run deploy:prod",
    "lint": "eslint src tests server",
    "lint:fix": "npm run lint -- --fix",
    "codecov": "cat coverage/*/lcov.info | codecov"
  },
  "betterScripts": {
    "compile": {
      "command": "babel-node bin/compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "dev": {
      "command": "nodemon --exec babel-node bin/server",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy": {
      "command": "npm run clean && npm run compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "deploy:dev": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy:prod": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "start": {
      "command": "babel-node bin/server",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "forever": {
      "command": "forever start -c node_modules/.bin/babel-node bin/server.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "test": {
      "command": "babel-node ./node_modules/karma/bin/karma start build/karma.conf",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": "app:*"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wolferian/wolferweb.git"
  },
